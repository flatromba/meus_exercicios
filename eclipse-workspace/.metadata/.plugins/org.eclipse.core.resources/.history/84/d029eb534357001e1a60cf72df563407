import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner input = new Scanner (System.in);
		double media1, media2, somaMedias, mediaMedias;
		media1=(8+9+7)/3;
		media2=(4+5+6)/3;
		somaMedias=media1+media2;
		mediaMedias=(media1+media2)/2;
		System.out.println("Media aritmética: "+media1+"\nMedia: "+media2+"\nSoma das médias: "+somaMedias+"\nMédia das médias: "+mediaMedias);
	}
}


